#include "aula0301.h"

ull CalcularTermoSerieFibonacci (us numeroTermos)
{
	
	if (numeroTermos <= 1)
		return numeroTermos;
	else
		return CalcularTermoSerieFibonacci (numeroTermos - 1) + CalcularTermoSerieFibonacci (numeroTermos - 2);
}


